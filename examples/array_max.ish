(defun array_max (arr)
  (var amax 0)
  (when (> (arrlen arr) 0)
    (progn
      (= amax (arrget arr 0))
      (loop (var i 1) (< i (arrlen arr)) (= i (+ i 1))
            (when (> (arrget arr i) amax)
              (= amax (arrget arr i))))))
  amax)

(defun test (ints)
  (print "arr: ")
  (println ints)
  (print "max: ")
  (println (array_max ints)))

(test (array -5 1 10 -8 2 9 5 3 -2 4))
(test (array -5 -3 -10 -4 -6))
