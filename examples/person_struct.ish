(struct Person
  (name
   age))

(defun isPerson (p)
  (isinstanceof p Person))

(defun makePerson (newName newAge)
  (var newPerson (makeinstance Person))
  (set newPerson name newName)
  (set newPerson age newAge)
  newPerson)

(defun printPerson (p)
  (print "[Person: name='")
  (print (get person name))
  (print "' age=")
  (print (get person age))
  (println "]"))

(var person (makePerson "Jon" 31))
(printPerson person)
(set person age 32)
(printPerson person)
