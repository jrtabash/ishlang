(defun array_map (ftn arr)
  (var ret (array))
  (foreach i arr (arrpush ret (ftn i)))
  ret)

(defun triple (x)
  (* 3 x))

(var ints (array 1 2 3 4 5))
(print "numbers: ")
(println ints)
(print "doubles: ")
(println (array_map (lambda (x) (* 2 x)) ints))
(print "triples: ")
(println (array_map triple ints))
