# ishlang development aliases
alias ishexportbuild='export LD_LIBRARY_PATH=build/'
alias ishexportbin='export LD_LIBRARY_PATH=/home/${USER}/bin'
alias ishshowpath='echo $LD_LIBRARY_PATH'
alias ishsource='cd ~/github/ishlang/src'
alias ishdebug='make DEBUG=1'
alias ishrelease='make DEBUG=0'
alias ishdebugtest='ishsource && cd unit_test && make DEBUG=1 && cd ..'
alias ishclean='make clean'
alias ishinstall='make install'
alias ishruntestsbin='ishsource && ishlang_unit_test && ishlang_run_tests.sh ~/bin/ishlang ~/bin/ishlang_test_files test_scenarios'
alias ishruntestsbuild='./build/ishlang_unit_test && ./test_runner/ishlang_run_tests.sh ./build/ishlang ./build/ishlang_test_files test_scenarios'
alias ishshowbin='ls -ltr ~/bin/ | grep ishlang'
alias ishprecommit='ishexportbin && ishshowpath && ishsource && ishrelease && ishinstall && ishclean && ishshowbin && ishruntestsbin'
