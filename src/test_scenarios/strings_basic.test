__CODE__
(var str "I am text")
(println str)
(println (istypeof str string))

;; length
(println (strlen str))

;; get
(println (strget str 0))
(println (strget str 3))
(println (strget str 8))

;; set
(strset str 5 'T')
(strset str 6 'E')
(strset str 7 'X')
(strset str 8 'T')
(println str)

__EXPECT__
I am text
true
9
I
m
t
I am TEXT
